

Sub OpenGeeksForGeeks()
    Dim ie As Object
    Dim url As String
    Dim buttons As Object
    Dim button As Object
    Dim i As Integer

    ' Define the URL to be opened
    url = "https://www.geeksforgeeks.org"

    ' Create an instance of Internet Explorer
    Set ie = CreateObject("InternetExplorer.Application")

    ' Make IE visible
    ie.Visible = True

    ' Navigate to the specified URL
    ie.Navigate url

    ' Wait until the IE browser has finished loading the page
    Do While ie.Busy Or ie.ReadyState <> 4
        DoEvents
    Loop

    ' Find elements by their class name
    Set buttons = ie.Document.getElementsByClassName("HomePageSectionHeader_homePageSectionHeader--cta__aRzJv")

    ' Loop through the elements and click the first element with the specified class name
    For i = 0 To buttons.Length - 1
        If buttons.Item(i).tagName = "A" Then
            Set button = buttons.Item(i)
            Exit For
        End If
    Next i

    ' Click the button if found
    If Not button Is Nothing Then
        button.Click
    Else
        MsgBox "Button not found!"
    End If

    ' Cleanup
    ' Uncomment the following lines if you want to close IE after opening the page
    ' ie.Quit
    ' Set ie = Nothing
End Sub

